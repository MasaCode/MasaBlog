extends layout

block append script
  link(rel='stylesheet', href='/stylesheets/post_v.css')
  script(src="/javascripts/index_v.js")
block header
  include include/header
block content
  div.container.container-main
    block activeVariable
      - var activeName="posts";
    include include/topMenu

    div.row
      div.col-md-8
        div.well.blog-post
          h3.post-title Blog Post One
          <p class="post-info"><i class="fa fa-calendar"></i> August 9th 2017 <i class="fa fa-tags"></i>  Web Development, Docker</p>
          hr.post-line
          img(src="/assets/images/blog1.jpg").img-responsive
