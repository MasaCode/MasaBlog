extends layout

block append script
  link(rel='stylesheet', href='/stylesheets/post_v.css')
  script(src="/javascripts/index_v.js")
block header
  header(style="background-image: url('/assets/images/homeCover.jpg');").bg-wrapper
    - var pageTitle = "Post Story one";
    - var pageDescription = '<p class="text-center post-info"><i class="fa fa-calendar"></i> August 9th 2017 <i class="fa fa-tags"></i> Webdevelopment, Docker</p>';
    include include/header
